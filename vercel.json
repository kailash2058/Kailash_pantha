{
    "version": 2,
    "builds": [
        {
            "src": "portfolio/portfolio/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/portfolio/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "portfolio/portfolio/wsgi.py"
        }
    ]
}
